[
  {
    "httpRequest": {
      "path": "/topic/{topic}",
      "pathParameters": {
        "topic": [
          {
            "schema": {
              "type": "string",
              "pattern": "^[a-zA-Z0-9-_]+$"
            }
          }
        ]
      }
    },
    "httpResponse": {
      "statusCode": 200
    }
  },
  {
    "httpRequest": {
      "path": "/"
    },
    "httpOverrideForwardedRequest": {
      "httpRequest": {
        "path": "/mockserver/dashboard"
      }
    }
  }
]
